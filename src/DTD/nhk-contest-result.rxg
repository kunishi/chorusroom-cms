<!DOCTYPE grammar PUBLIC "-//RELAX//DTD RELAX Namespace 1.0//JA" "/home/kunishi/share/xml/relax/relaxNamespace.dtd">
<grammar relaxNamespaceVersion="1.0" grammarVersion="1.0" xmlns="http://www.xml.gr.jp/xmlns/relaxNamespace">

  <topLevel>
    <ref label="competition" namespace="http://www.chorusroom.org/xml"/>
  </topLevel>

  <module location="nhk-contest-result.rxm" namespace="http://www.chorusroom.org/xml"/>
  <module location="choir-piece.rxm" namespace="http://www.chorusroom.org/piece">
  </module>
  <module location="utf8-char.rxm" namespace="http://www.chorusroom.org/character">
  </module>

  <div>
    <hedgeRule label="choirPiece" namespace="http://www.chorusroom.org/xml">
      <choice occurs="?">
	<ref label="piece" namespace="http://www.chorusroom.org/piece"/>
	<ref label="suite" namespace="http://www.chorusroom.org/piece"/>
      </choice>
    </hedgeRule>
    <hedgeRule label="specialCharacters"
	       namespace="http://www.chorusroom.org/xml">
      <ref label="utf8-char" namespace="http://www.chorusroom.org/character"/>
    </hedgeRule>
  </div>
  <div>
    <hedgeRule label="specialCharacters"
	       namespace="http://www.chorusroom.org/piece">
      <ref label="utf8-char"
	   namespace="http://www.chorusroom.org/character"/>
    </hedgeRule>
  </div>

</grammar>
