<?xml version="1.0"?>
<grammar ns="http://www.chorusroom.org/xml"
    xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
    <start>
        <element name="competition">
            <optional>
                <attribute name="xml:lang">
                    <data type="string"/>
                </attribute>
            </optional>
            <element name="cvs-id">
                <data type="string"/>
            </element>
            <element name="competition-name">
                <optional>
                    <attribute name="pref">
                        <choice>
                            <value type="NMTOKEN">true</value>
                            <value type="NMTOKEN">false</value>
                        </choice>
                    </attribute>
                </optional>
                <optional>
                    <attribute name="area">
                        <choice>
                            <value type="NMTOKEN">true</value>
                            <value type="NMTOKEN">false</value>
                        </choice>
                    </attribute>
                </optional>
                <mixed>
                    <optional>
                        <ref name="utf8-char"/>
                    </optional>
                </mixed>
            </element>
            <oneOrMore>
                <element name="result">
                    <attribute name="output">
                        <data type="string"/>
                    </attribute>
                    <zeroOrMore>
                        <element name="date">
                            <attribute name="day-of-week">
                                <choice>
                                    <value type="NMTOKEN">wed</value>
                                    <value type="NMTOKEN">sat</value>
                                    <value type="NMTOKEN">fri</value>
                                    <value type="NMTOKEN">sun</value>
                                    <value type="NMTOKEN">thu</value>
                                    <value type="NMTOKEN">tue</value>
                                    <value type="NMTOKEN">mon</value>
                                </choice>
                            </attribute>
                            <optional>
                                <attribute name="holiday">
                                    <choice>
                                    <value type="NMTOKEN">true</value>
                                    <value type="NMTOKEN">false</value>
                                    </choice>
                                </attribute>
                            </optional>
                            <data type="date"/>
                        </element>
                    </zeroOrMore>
                    <optional>
                        <element name="startTime">
                            <data type="time"/>
                        </element>
                    </optional>
                    <optional>
                        <element name="hall">
                            <mixed>
                                <optional>
                                    <ref name="utf8-char"/>
                                </optional>
                            </mixed>
                        </element>
                    </optional>
                    <zeroOrMore>
                        <element name="referee">
                            <optional>
                                <attribute name="xml:lang">
                                    <data type="string"/>
                                </attribute>
                            </optional>
                            <attribute name="shortname">
                                <data type="string"/>
                            </attribute>
                            <optional>
                                <attribute name="job">
                                    <data type="string"/>
                                </attribute>
                            </optional>
                            <optional>
                                <attribute name="referred-date">
                                    <data type="string"/>
                                </attribute>
                            </optional>
                            <optional>
                                <attribute name="country">
                                    <data type="string"/>
                                </attribute>
                            </optional>
                            <mixed>
                                <optional>
                                    <ref name="utf8-char"/>
                                </optional>
                            </mixed>
                        </element>
                    </zeroOrMore>
                    <oneOrMore>
                        <element name="reporter">
                            <optional>
                                <attribute name="anonymous">
                                    <choice>
                                    <value type="NMTOKEN">true</value>
                                    <value type="NMTOKEN">false</value>
                                    </choice>
                                </attribute>
                            </optional>
                            <element name="reporter-name">
                                <optional>
                                    <attribute name="xml:lang">
                                    <data type="string"/>
                                    </attribute>
                                </optional>
                                <optional>
                                    <attribute name="penname">
                                    <data type="string"/>
                                    </attribute>
                                </optional>
                                <mixed>
                                    <optional>
                                    <ref name="utf8-char"/>
                                    </optional>
                                </mixed>
                            </element>
                            <element name="reporter-email">
                                <optional>
                                    <attribute name="anonymous">
                                    <choice>
                                    <value type="NMTOKEN">true</value>
                                    <value type="NMTOKEN">false</value>
                                    </choice>
                                    </attribute>
                                </optional>
                                <data type="string"/>
                            </element>
                        </element>
                    </oneOrMore>
                    <optional>
                        <element name="notices">
                            <oneOrMore>
                                <element name="notice">
                                    <mixed>
                                    <optional>
                                    <ref name="utf8-char"/>
                                    </optional>
                                    </mixed>
                                </element>
                            </oneOrMore>
                        </element>
                    </optional>
                    <oneOrMore>
                        <element name="section">
                            <element name="section-name">
                                <optional>
                                    <attribute name="date">
                                    <data type="string"/>
                                    </attribute>
                                </optional>
                                <mixed>
                                    <optional>
                                    <ref name="utf8-char"/>
                                    </optional>
                                </mixed>
                            </element>
                            <oneOrMore>
                                <element name="choir">
                                    <attribute name="representative">
                                    <choice>
                                    <value type="NMTOKEN">true</value>
                                    <value type="NMTOKEN">false</value>
                                    </choice>
                                    </attribute>
                                    <attribute name="seed">
                                    <choice>
                                    <value type="NMTOKEN">true</value>
                                    <value type="NMTOKEN">false</value>
                                    </choice>
                                    </attribute>
                                    <optional>
                                    <attribute name="playing-order">
                                    <data type="positiveInteger"/>
                                    </attribute>
                                    </optional>
                                    <element name="choir-name">
                                    <optional>
                                    <attribute name="xml:lang">
                                    <data type="string"/>
                                    </attribute>
                                    </optional>
                                    <optional>
                                    <attribute name="url">
                                    <data type="anyURI"/>
                                    </attribute>
                                    </optional>
                                    <mixed>
                                    <optional>
                                    <ref name="utf8-char"/>
                                    </optional>
                                    </mixed>
                                    </element>
                                    <optional>
                                    <element name="prefecture">
                                    <data type="string"/>
                                    </element>
                                    </optional>
                                    <optional>
                                    <element name="choir-type">
                                    <choice>
                                    <value type="NMTOKEN">same-voice</value>
                                    <value type="NMTOKEN">female</value>
                                    <value type="NMTOKEN">male</value>
                                    <value type="NMTOKEN">children</value>
                                    <value type="NMTOKEN">mixed</value>
                                    </choice>
                                    </element>
                                    </optional>
                                    <optional>
                                    <element name="number-of-members">
                                    <data type="positiveInteger"/>
                                    </element>
                                    </optional>
                                    <zeroOrMore>
                                    <choice>
                                    <element name="conductor">
                                    <optional>
                                    <attribute name="xml:lang">
                                    <data type="string"/>
                                    </attribute>
                                    </optional>
                                    <optional>
                                    <attribute name="instrument">
                                    <data type="string"/>
                                    </attribute>
                                    </optional>
                                    <mixed>
                                    <optional>
                                    <ref name="utf8-char"/>
                                    </optional>
                                    </mixed>
                                    </element>
                                    <element name="piano">
                                    <optional>
                                    <attribute name="xml:lang">
                                    <data type="string"/>
                                    </attribute>
                                    </optional>
                                    <mixed>
                                    <optional>
                                    <ref name="utf8-char"/>
                                    </optional>
                                    </mixed>
                                    </element>
                                    <element name="accompaniment">
                                    <optional>
                                    <attribute name="xml:lang">
                                    <data type="string"/>
                                    </attribute>
                                    </optional>
                                    <attribute name="instrument">
                                    <data type="string"/>
                                    </attribute>
                                    <mixed>
                                    <optional>
                                    <ref name="utf8-char"/>
                                    </optional>
                                    </mixed>
                                    </element>
                                    </choice>
                                    </zeroOrMore>
                                    <element name="prize">
                                    <attribute name="nickname">
                                    <choice>
                                    <value type="NMTOKEN">blonze</value>
                                    <value type="NMTOKEN">seeded</value>
                                    <value type="NMTOKEN">silver</value>
                                    <value type="NMTOKEN">none</value>
                                    <value type="NMTOKEN">gold</value>
                                    </choice>
                                    </attribute>
                                    <data type="string"/>
                                    </element>
                                    <zeroOrMore>
                                    <element name="special-prize">
                                    <data type="string"/>
                                    </element>
                                    </zeroOrMore>
                                    <optional>
                                    <element name="program">
                                    <optional>
                                    <element name="given-program">
                                    <choice>
                                    <empty/>
                                    <ref name="pieceRef"/>
                                    <ref name="piece"/>
                                    <ref name="suite"/>
                                    </choice>
                                    <optional>
                                    <ref name="programNote"/>
                                    </optional>
                                    </element>
                                    </optional>
                                    <oneOrMore>
                                    <element name="free-program">
                                    <choice>
                                    <empty/>
                                    <ref name="pieceRef"/>
                                    <ref name="piece"/>
                                    <ref name="suite"/>
                                    </choice>
                                    <optional>
                                    <ref name="programNote"/>
                                    </optional>
                                    </element>
                                    </oneOrMore>
                                    </element>
                                    </optional>
                                    <optional>
                                    <element name="scores">
                                    <oneOrMore>
                                    <element name="score">
                                    <attribute name="referee">
                                    <data type="string"/>
                                    </attribute>
                                    <data type="string"/>
                                    </element>
                                    </oneOrMore>
                                    <element name="total-score">
                                    <data type="string"/>
                                    </element>
                                    <element name="score-note">
                                    <data type="string"/>
                                    </element>
                                    </element>
                                    </optional>
                                    <optional>
                                    <element name="choir-note">
                                    <data type="string"/>
                                    </element>
                                    </optional>
                                </element>
                            </oneOrMore>
                        </element>
                    </oneOrMore>
                </element>
            </oneOrMore>
        </element>
    </start>
    <define name="pieceRef">
        <element name="pieceRef">
            <attribute name="number">
                <choice>
                    <value type="NMTOKEN">M3</value>
                    <value type="NMTOKEN">M1</value>
                    <value type="NMTOKEN">G2</value>
                    <value type="NMTOKEN">F1</value>
                    <value type="NMTOKEN">M4</value>
                    <value type="NMTOKEN">G3</value>
                    <value type="NMTOKEN">F2</value>
                    <value type="NMTOKEN">G4</value>
                    <value type="NMTOKEN">F3</value>
                    <value type="NMTOKEN">M2</value>
                    <value type="NMTOKEN">F4</value>
                    <value type="NMTOKEN">G1</value>
                </choice>
            </attribute>
            <attribute name="href">
                <data type="anyURI"/>
            </attribute>
        </element>
    </define>
    <define name="title">
        <element>
            <name ns="http://www.chorusroom.org/piece">title</name>
            <optional>
                <attribute name="original">
                    <choice>
                        <value type="string">true</value>
                        <value type="string">false</value>
                    </choice>
                </attribute>
            </optional>
            <optional>
                <attribute name="xml:lang">
                    <ref name="string"/>
                </attribute>
            </optional>
            <optional>
                <element>
                    <name ns="http://www.chorusroom.org/piece">titlePrefix</name>
                    <mixed>
                        <ref name="utf8-char"/>
                    </mixed>
                </element>
            </optional>
            <element>
                <name ns="http://www.chorusroom.org/piece">mainTitle</name>
                <mixed>
                    <ref name="utf8-char"/>
                </mixed>
            </element>
            <optional>
                <element>
                    <name ns="http://www.chorusroom.org/piece">titleSuffix</name>
                    <mixed>
                        <ref name="utf8-char"/>
                    </mixed>
                </element>
            </optional>
        </element>
    </define>
    <define name="originated-from">
        <element>
            <name ns="http://www.chorusroom.org/piece">originated-from</name>
            <optional>
                <attribute name="xml:lang">
                    <ref name="string"/>
                </attribute>
            </optional>
            <mixed>
                <ref name="utf8-char"/>
            </mixed>
        </element>
    </define>
    <define name="suite">
        <element>
            <name ns="http://www.chorusroom.org/piece">suite</name>
            <optional>
                <ref name="originated-from"/>
            </optional>
            <optional>
                <ref name="words-by"/>
            </optional>
            <optional>
                <ref name="translated-by"/>
            </optional>
            <optional>
                <ref name="composed-by"/>
            </optional>
            <optional>
                <ref name="arranged-by"/>
            </optional>
            <oneOrMore>
                <ref name="title"/>
            </oneOrMore>
            <oneOrMore>
                <element>
                    <name ns="http://www.chorusroom.org/piece">suite-piece</name>
                    <oneOrMore>
                        <choice>
                            <element>
                                <name ns="http://www.chorusroom.org/piece">piece-number</name>
                                <mixed>
                                    <ref name="utf8-char"/>
                                </mixed>
                            </element>
                            <ref name="piece"/>
                            <ref name="suite"/>
                        </choice>
                    </oneOrMore>
                </element>
            </oneOrMore>
            <optional>
                <ref name="piece-note"/>
            </optional>
        </element>
    </define>
    <define name="string">
        <data type="string"/>
    </define>
    <define name="piece">
        <element>
            <name ns="http://www.chorusroom.org/piece">piece</name>
            <optional>
                <ref name="originated-from"/>
            </optional>
            <optional>
                <ref name="words-by"/>
            </optional>
            <optional>
                <ref name="translated-by"/>
            </optional>
            <optional>
                <ref name="composed-by"/>
            </optional>
            <optional>
                <ref name="arranged-by"/>
            </optional>
            <oneOrMore>
                <ref name="title"/>
            </oneOrMore>
            <optional>
                <ref name="piece-note"/>
            </optional>
        </element>
    </define>
    <define name="words-by">
        <element>
            <name ns="http://www.chorusroom.org/piece">words-by</name>
            <optional>
                <attribute name="xml:lang">
                    <ref name="string"/>
                </attribute>
            </optional>
            <optional>
                <attribute name="kind">
                    <data type="string"/>
                </attribute>
            </optional>
            <mixed>
                <ref name="utf8-char"/>
            </mixed>
        </element>
    </define>
