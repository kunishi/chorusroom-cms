    <xs:attributeGroup ref="Common.attrib"/>
  </xs:attributeGroup>
  <xs:complexType name="Heading.content">
    <xs:complexContent>
      <xs:extension base="xhtml:Inline.model"/>
    </xs:complexContent>
  </xs:complexType>
  <xs:element name="Heading.class" abstract="true" substitutionGroup="xhtml:Block.class">
    <xs:complexType>
      <xs:complexContent>
        <xs:extension base="xhtml:Heading.content">
          <xs:attributeGroup ref="xhtml:Heading.attrib"/>
        </xs:extension>
      </xs:complexContent>
    </xs:complexType>
  </xs:element>
  <xs:element name="Block.class" abstract="true" substitutionGroup="xhtml:Block.mix"/>
  <xs:element name="Inline.class" abstract="true"/>
  <xs:complexType name="Inline.model" mixed="true">
    <xs:sequence>
      <xs:element minOccurs="0" maxOccurs="unbounded" ref="xhtml:Inline.class"/>
    </xs:sequence>
  </xs:complexType>
  <!-- This is redefined by the legacy module to include inlines. -->
  <xs:element name="Block.mix" abstract="true"/>
  <xs:complexType name="Block.model">
    <xs:sequence>
      <xs:element maxOccurs="unbounded" ref="xhtml:Block.mix"/>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Flow.model" mixed="true">
    <xs:choice minOccurs="0" maxOccurs="unbounded">
      <xs:element ref="xhtml:Inline.class"/>
      <xs:element ref="xhtml:Block.class"/>
    </xs:choice>
  </xs:complexType>
